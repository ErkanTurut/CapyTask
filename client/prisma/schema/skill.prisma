model skill {
    id                     String                   @id @default(dbgenerated("nanoid(10)"))
    name                   String
    description            String?
    created_at             DateTime                 @default(now()) @db.Timestamptz(6)
    updated_at             DateTime                 @default(now()) @updatedAt @db.Timestamptz(6)
    service_resource_skill service_resource_skill[]

    @@index([name])
}
